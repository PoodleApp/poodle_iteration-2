{
  "name": "poodle-service",
  "version": "0.1.3",
  "description": "Abstracts IMAP interactions, such as downloading entire threads; provides transparent caching",
  "main": "lib/index.js",
  "repository": {
    "url": "https://github.com/PoodleApp/poodle-service",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/PoodleApp/poodle-service/issues"
  },
  "homepage": "https://github.com/PoodleApp/poodle-service#readme",
  "author": "Jesse Hallett <jesse@sitr.us>",
  "license": "Apache-2.0",
  "scripts": {
    "test": "ava",
    "build": "make",
    "build:watch": "watch 'yarn run build' src",
    "prepublishOnly": "npm test && npm run build",
    "postinstall": "../../scripts/postinstall"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread"
    ],
    "presets": [
      "@babel/preset-flow",
      [
        "@babel/preset-env",
        {
          "targets": {
            "electron": "1.8"
          }
        }
      ]
    ]
  },
  "ava": {
    "color": false,
    "files": [
      "test/**/*_test.js"
    ],
    "sources": [
      "src/**/*.js"
    ],
    "require": [
      "@babel/register"
    ]
  },
  "dependencies": {
    "activitystrea.ms": "hallettj/activitystrea.ms#fix/template-idempotency",
    "arfe": "*",
    "array-unique": "^0.3.2",
    "base64-stream": "^0.1.3",
    "blob-to-stream": "^1.0.3",
    "cache-directory": "^1.1.0",
    "dateformat": "^2.0.0",
    "deep-equal": "^1.0.1",
    "googleapis": "^14.2.0",
    "imap": "^0.8.18",
    "libqp": "^1.1.0",
    "mailparser": "^2.0.1",
    "mkdirp": "^0.5.1",
    "moment": "^2.18.1",
    "mori": "^0.3.2",
    "node-fetch": "^1.6.3",
    "nodemailer": "^4.0.1",
    "process-nextick-args": "^1.0.7",
    "sanitize-filename": "^1.6.1",
    "stream-to-blob": "^1.0.0",
    "stream-to-string": "^1.1.0",
    "uuid": "^3.1.0",
    "xoauth2": "^1.2.0"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.44",
    "@babel/core": "7.0.0-beta.44",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.44",
    "@babel/preset-env": "7.0.0-beta.44",
    "@babel/preset-flow": "7.0.0-beta.44",
    "@babel/register": "7.0.0-beta.44",
    "ava": "1.0.0-beta.4",
    "flow-bin": "^0.73.0",
    "format-json": "^1.0.3"
  },
  "files": [
    "lib/"
  ],
  "peerDependencies": {
    "kefir": "^3.8.1"
  }
}
