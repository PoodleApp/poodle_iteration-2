{
  "name": "redux-slurp",
  "version": "0.0.0",
  "description": "Redux helper to stream data from an observable to a React component",
  "main": "index.js",
  "repository": "git+https://github.com/PoodleApp/poodle.git",
  "author": "Jesse Hallett <jesse@sitr.us>",
  "license": "Apache-2.0",
  "scripts": {
    "test": "ava",
    "build": "make",
    "build:watch": "watch 'yarn run build' src",
    "prepublishOnly": "flow && yarn test && yarn run build",
    "prepublish": "(not-in-publish && echo 'Skipping prepublish') || npm run require-npm4-to-publish",
    "require-npm4-to-publish": "semver -r '>=4.0.0' $(npm --version) || (echo 'NPM 4+ required to publish' && exit 1)"
  },
  "babel": {
    "presets": [
      "react",
      "stage-3",
      [
        "env",
        {
          "targets": {
            "electron": "1.6"
          }
        }
      ]
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit",
    "color": false
  },
  "dependencies": {
    "is-observable": "*",
    "symbol-observable": "*"
  },
  "devDependencies": {
    "ava": "^0.20.0",
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "flow-bin": "^0.49.1",
    "in-publish": "^2.0.0",
    "kefir": "^3.7.2",
    "react": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-test-renderer": "^15.6.1",
    "redux": "^3.7.1",
    "redux-fractal": "^1.7.1",
    "watch": "^1.0.2"
  },
  "files": [
    "lib/"
  ],
  "peerDependencies": {
    "react": "*",
    "react-redux": "*",
    "redux": "*",
    "redux-fractal": "*"
  }
}
