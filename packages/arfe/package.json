{
  "name": "arfe",
  "version": "0.1.0",
  "description": "Federated social protocol on top of email",
  "scripts": {
    "test": "ava",
    "build": "make",
    "build:watch": "watch 'yarn run build' src",
    "prepublishOnly": "npm test && npm run build",
    "postinstall": "../../scripts/postinstall"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PoodleApp/arfe.git"
  },
  "keywords": [
    "social",
    "email"
  ],
  "author": "Jesse Hallett <jesse@sitr.us>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/PoodleApp/arfe/issues"
  },
  "homepage": "https://github.com/PoodleApp/arfe#readme",
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread"
    ],
    "presets": [
      "@babel/preset-flow",
      [
        "@babel/preset-env",
        {
          "targets": {
            "electron": "1.8"
          }
        }
      ]
    ]
  },
  "ava": {
    "color": false,
    "files": [
      "test/**/*_test.js"
    ],
    "sources": [
      "src/**/*.js"
    ],
    "require": [
      "@babel/register"
    ]
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.44",
    "@babel/core": "7.0.0-beta.44",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.44",
    "@babel/preset-env": "7.0.0-beta.44",
    "@babel/preset-flow": "7.0.0-beta.44",
    "@babel/register": "7.0.0-beta.44",
    "ava": "1.0.0-beta.4",
    "flow-bin": "^0.73.0",
    "libmime": "^3.1.0",
    "string-to-stream": "^1.1.0"
  },
  "dependencies": {
    "activitystrea.ms": "hallettj/activitystrea.ms#fix/template-idempotency",
    "activitystreams-context": "^1.0.0",
    "buildmail": "^4.0.1",
    "email-addresses": "^3.0.1",
    "media-type": "^0.3.1",
    "moment": "^2.17.1",
    "mori": "^0.3.2",
    "node-uuid": "^1.4.8",
    "stream-from-promise": "^1.0.0",
    "stream-to-string": "^1.1.0",
    "traverse": "^0.6.6",
    "vocabs-as": "^2"
  },
  "files": [
    "lib/"
  ]
}
